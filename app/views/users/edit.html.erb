<%= form_for @user do |f| %>
    <% if @user.errors.any? %>
      <%= @user.errors.full_messages %>
      <br>
    <% end %>
    
	<%= f.label :username %>
 	<%= f.text_field(:username) %>
 	<br>
 	<%= f.label :email %>
 	<%= f.email_field(:email) %>
 	<br>
 	<%= f.label :password %>
 	<%= f.password_field :password %>
 	<br>
 	<%= f.submit %>
<% end %>